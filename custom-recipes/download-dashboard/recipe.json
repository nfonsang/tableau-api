{
    "meta": {
        "label": "Download Tableau Dashboard",
        "description": "This recipe downloads a Tableau dashboard into a folder",
        "icon": "icon-download"
    },
    
    // kind of plugin recipe
    "kind": "PYTHON",
    
    // the output of this recipe component 
    "outputRoles": [
        {
            "name": "output_folder",
            "label": "Output Folder",
            "description": "This is the folder where the downloaded Tableau dashboard view content will be saved",
            "arity": "UNARY",
            "required": false,
            "acceptsDataset": false
        },
    ],

    // parameters 
    
username = variables['local']["username"]
password = variables['local']["pw"]
sitename = "nebatesting"
server_url = "https://us-west-2b.online.tableau.com/"
api_version = "3.16"
# view_name = "Stocks"
view_id = "37b94338-5aa3-4887-82c7-b355e6ac3525"
folder = dataiku.Folder("image_folder")

# filter parameter keys and values 
filters = ["Company", "Amazon,Apple", "YEAR(Date)", "2013"]

    "params": [
        {
            "name": "server_url",
            "label": "Tableau Base Server URL",
            "type": "STRING",
            "description": "This is URL of the Tableau Server, for example: https://us-west-2b.online.tableau.com/"
            "mandatory": true
        },

        {
            "name": "site_id",
            "label": "Tableau Site ID",
            "type": "STRING",
            "description": "This is the name or ID Tableau site. For example,  tableau_server_url/#/site/site_id/views
            "mandatory": true
        },

        {
            "name": "parameter2",
            "type": "INT",
            "defaultValue": 42
            /* Note that standard json parsing will return it as a double in Python (instead of an int), so you need to write
               int(get_recipe_config()['parameter2'])
            */
        },

        // A "SELECT" parameter is a multi-choice selector. Choices are specified using the selectChoice field
        {
            "name": "parameter3",
            "type": "SELECT",
            "selectChoices": [
                {
                    "value": "val_x",
                    "label": "display name for val_x"
                },
                {
                    "value": "val_y",
                    "label": "display name for val_y"
                }
            ]
        },

        // A 'COLUMN' parameter is a string, whose value is a column name from an input schema.
        // To specify the input schema whose column names are used, use the "columnRole" field like below.
        // The column names will come from the schema of the first dataset associated to that role.
        {
            "name": "parameter4",
            "type": "COLUMN",
            "columnRole": "input_B_role"
        }

        // The 'COLUMNS' type works in the same way, except that it is a list of strings.
    ],

    // The field "resourceKeys" holds a list of keys that allows to limit the number
    // of concurrent executions and activities triggered by this recipe.
    //
    // Administrators can configure the limit per resource key in the Administration > Settings > Flow build
    // screen.

    "resourceKeys": []

}
